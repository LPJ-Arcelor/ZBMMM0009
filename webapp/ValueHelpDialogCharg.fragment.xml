<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialogCharg"
		title="Lotes"
		items="{/ShMatnrChargSet}"
		search=".onValueHelpSearchCharg"
		confirm=".onValueHelpCloseCharg"
		cancel=".onValueHelpCloseCharg">
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{Charg}"
			description="{ path: 'Clabs', formatter: '.formatter.currencyValue' }" /> 
	</SelectDialog>
</core:FragmentDefinition>