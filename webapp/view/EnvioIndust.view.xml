  xmlns:m="sap.m"
  xmlns:t="sap.ui.table"
  xmlns:core="sap.ui.core"
  xmlns:form="sap.ui.layout.form"
  xmlns:layout="sap.ui.layout">

  <ObjectPageLayout xmlns="sap.uxap" id="page" upperCaseAnchorBar="false" flexEnabled="true" preserveHeaderStateOnScroll="true" showFooter="true" showHeaderContent="true" busyIndicatorDelay="0" useIconTabBar="true">

    <headerTitle>
      <ObjectPageHeader id="header" objectTitle="{i18n>EnvioIndust}" showTitleSelector="false">
      </ObjectPageHeader>
    </headerTitle>

    <sections>

      <!--
        ************ MANUAL ************
      -->
      <ObjectPageSection titleUppercase="false" id="pageSectionManual" title="{i18n>lblEnvioManual}">
        <ObjectPageSubSection id="sectionManualHeader" title="{i18n>lblData}" titleUppercase="false">
          <blocks>
            <m:VBox class="sapUiSmallMargin">
              <form:SimpleForm layout="ResponsiveGridLayout" width="70%">
                <core:Title text="Informar Dados"/>
                <Label xmlns="sap.m" text="Centro" id="lblCentro"/>
                <Select id="inWerks" change=".onChangeCentro" forceSelection="false"
                  xmlns="sap.m" showSecondaryValues="true" wrapItemsText="true" width="47%" selectedKey="{ItensIndust_JSONModel>/Werks}" items="{path:'/ShPlantSet', templateShareable:true}">
                  <core:ListItem key="{Plant}" text="{Plant} - {Name1}" />
                </Select>
                <Label xmlns="sap.m" text="Fornecedor" id="lblFornecedor"/>
                <m:Input id="inLifnr" width="95%" placeholder="Selecionar Fornecedor" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequestLifnr" suggestionItems="{
										path: '/ShFornecedorSet',
										sorter: { path: 'Name1' } }">
                  <core:Item key="{Fornecedor}" text="{Name1} ({Fornecedor})" />
                </m:Input>
                <Text xmlns="sap.m" id="lblDescFornecedor" width="100%"/>
              </form:SimpleForm>
            </m:VBox>
          </blocks>
        </ObjectPageSubSection>
        <ObjectPageSubSection id="sectionManualItems" title="{i18n>lblItems}" titleUppercase="false">
          <blocks>
            <t:Table id="ItensEnvIndustTable" rows="{ path: 'ItensIndust_JSONModel>/Itens' }" selectionMode="MultiToggle" fixedColumnCount="1" visibleRowCountMode="Auto" minAutoRowCount="10">
              <t:extension>
                <OverflowToolbar xmlns="sap.m" id="toolbar1" width="100%">
                  <ToolbarSpacer id="spacer0"/>
                  <m:Button id="btnNovo" text="Novo" type="Transparent" icon="sap-icon://write-new-document" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="onNovo"/>
                  <m:Button id="btnDel" text="Eliminar" type="Transparent" icon="sap-icon://delete" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="onDel"/>
                </OverflowToolbar>
              </t:extension>
              <t:columns>
                <t:Column id="column1" width="50px">
                  <Label xmlns="sap.m" text="Item" id="label1"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{ItensIndust_JSONModel>Item}" wrapping="false" />
                  </t:template>
                </t:Column>
                <t:Column id="column2" width="200px">
                  <Label xmlns="sap.m" text="Material" id="label2"/>
                  <t:template>
                    <Input id="productInput"
                      xmlns="sap.m" value="{ItensIndust_JSONModel>Matnr}" placeholder="Selecionar Material" showSuggestion="true" showValueHelp="true" change=".onvhMatnrChange" valueHelpRequest=".onValueHelpRequest">
                    </Input>
                  </t:template>
                </t:Column>
                <t:Column id="column3" width="310px">
                  <Label xmlns="sap.m" text="Descrição Material" id="label3"/>
                  <t:template>
                    <Text xmlns="sap.m" id="txtMaktx" text="{ItensIndust_JSONModel>Maktx}" width="300px" />
                  </t:template>
                </t:Column>
                <t:Column id="column4" width="120px">
                  <Label xmlns="sap.m" text="Depósito" id="label4"/>
                  <t:template>
                    <Input id="inLgort"
                      xmlns="sap.m" value="{ItensIndust_JSONModel>Lgort}" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequestLgort">
                    </Input>
                  </t:template>
                </t:Column>
                <t:Column id="column5" width="100px">
                  <Label xmlns="sap.m" text="Qtde Saída" id="label5"/>
                  <t:template>
                    <!--<Input xmlns="sap.m" value="{path:'ItensIndust_JSONModel>Menge' }" type="Number" required="true" />-->
                    <Input xmlns="sap.m" value="{path: 'ItensIndust_JSONModel>Menge', type:'sap.ui.model.type.Float', formatOptions:{minFractionDigits:3,maxFractionDigits:3}, constraints:{precision:13,scale:3}}"/>
                  </t:template>
                </t:Column>
                <t:Column id="column6" width="150px">
                  <Label xmlns="sap.m" text="Lote" id="label6"/>
                  <t:template>
                    <Input id="inCharg"
                      xmlns="sap.m" value="{ItensIndust_JSONModel>Charg}" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequestCharg">
                    </Input>
                  </t:template>
                </t:Column>
                <t:Column width="70px">
                  <Label xmlns="sap.m" text="UM básica"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{ItensIndust_JSONModel>Meins}" />
                  </t:template>
                </t:Column>
                <t:Column id="column8" width="150px">
                  <Label xmlns="sap.m" text="Pedido" id="label8"/>
                  <t:template>
                    <Input xmlns="sap.m" maxLength="10" value="{ItensIndust_JSONModel>Ebeln}" change=".onChangePedidoItem"/>
                  </t:template>
                </t:Column>
                <t:Column id="column9" width="100px">
                  <Label xmlns="sap.m" text="Item Pedido" id="label9"/>
                  <t:template>
                    <Input xmlns="sap.m" maxLength="5" value="{ItensIndust_JSONModel>Ebelp}" change=".onChangePedidoItem" />
                  </t:template>
                </t:Column>
                <t:Column width="200px">
                  <Label xmlns="sap.m" text="Material acabado"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{ItensIndust_JSONModel>MaterialAcabado}" width="100px" />
                  </t:template>
                </t:Column>
                <t:Column width="310px">
                  <Label xmlns="sap.m" text="Descrição Material acabado" />
                  <t:template>
                    <Text xmlns="sap.m" text="{ItensIndust_JSONModel>MaterialAcabadoDesc}" width="300px" />
                  </t:template>
                </t:Column>
              </t:columns>
            </t:Table>
          </blocks>
        </ObjectPageSubSection>
      </ObjectPageSection>

      <!--
        ************ PEDIDO ************
      -->
