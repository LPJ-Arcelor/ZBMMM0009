                <t:Column width="100px">
                  <Label xmlns="sap.m" text="Qtde pedido"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{ path: 'envioX41Model>QtdPedido', formatter: '.formatter.qtyValue' }"/>
                  </t:template>
                </t:Column>
                <t:Column width="5em">
                  <Label xmlns="sap.m" text="UM básica"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioX41Model>UmBasica}"/>
                  </t:template>
                </t:Column>
                <t:Column width="100px">
                  <Label xmlns="sap.m" text="Qtde disponível"/>
                  <t:template>
                    <!--<Input xmlns="sap.m" id="fldQtdMatAcabadoManual" value="{ path: 'envioPedidoModel>QtdLote' }" type="Number"/>-->
                    <Input xmlns="sap.m" id="fldQtdMatAcabadoManualX41" value="{path: 'envioX41Model>QtdLote', type:'sap.ui.model.type.Float', formatOptions:{minFractionDigits:3,maxFractionDigits:3}, constraints:{precision:13,scale:3}}"/>
                  </t:template>
                </t:Column>
                <t:Column width="150px">
                  <Label xmlns="sap.m" text="Lote"/>
                  <t:template>
                    <Input id="inChargX41"
                      xmlns="sap.m" value="{envioX41Model>Lote}" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequestCharg">
                    </Input>
                  </t:template>
                </t:Column>
              </t:columns>
            </t:Table>
          </blocks>
        </ObjectPageSubSection>       
      </ObjectPageSection>

      <!--
        ************ CONTRATO ************
      -->
      <ObjectPageSection titleUppercase="false" id="pageSectionContrato" title="{i18n>lblEnvioContrato}">
        <ObjectPageSubSection id="sectionContratoHeader" title="{i18n>lblData}" titleUppercase="false">
          <blocks>
            <m:VBox class="sapUiSmallMargin">
              <form:SimpleForm layout="ResponsiveGridLayout" width="70%">
                <core:Title text="Informar Dados"/>
                <m:Label text="{i18n>lblContrato}"/>
                <m:Input id="fldContratoSearch" width="200em" required="true" maxLength="10" value="{path: 'envioContratoModel>/Contrato', type:'sap.ui.model.odata.type.String', constraints:{maxLength:10}}"/>
                <m:Label text="{i18n>lblItem}"/>
                <!-- <m:Input id="fldContratoItemSearch" width="75em" required="false" maxLength="5" value="{path: 'envioContratoModel>/Item', type:'sap.ui.model.odata.type.String', constraints:{maxLength:5}}"/> -->
                <m:MultiInput id="fldContratoItemMulti" required="false" showValueHelp="false" width="200em"/> 
                <Label xmlns="sap.m" text="Centro"/>
                <Select id="inWerksContrato" forceSelection="false" editable="false"
                  xmlns="sap.m" showSecondaryValues="true" wrapItemsText="true" width="47%" selectedKey="{envioContratoModel>/Werks}" items="{path:'/ShPlantSet', templateShareable:true}">
                  <core:ListItem key="{Plant}" text="{Plant} - {Name1}" />
                </Select>
                <Label xmlns="sap.m" text="Fornecedor"/>
                <m:Input id="inLifnrContrato" width="95%" editable="false" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequestLifnr" suggestionItems="{
										path: '/ShFornecedorSet',
										sorter: { path: 'Name1' } }">
                  <core:Item key="{Fornecedor}" text="{Name1} ({Fornecedor})" />
                </m:Input>
                <Text xmlns="sap.m" id="lblDescFornecedorContrato" width="100%"/>
              </form:SimpleForm>
              <layout:HorizontalLayout class="sapUiContentPadding">
                <m:Button type="Default" text="{i18n>btnAddItemsContrato}" icon="sap-icon://add-document" press="onSearchContrato" />
              </layout:HorizontalLayout>
            </m:VBox>
          </blocks>
        </ObjectPageSubSection>
        <ObjectPageSubSection id="sectionContratoItems" title="{i18n>lblItems}" titleUppercase="false">
          <blocks>
            <t:Table id="ItensContratoTable" rows="{ path: 'envioContratoModel>/Itens' }" selectionMode="MultiToggle" fixedColumnCount="2" visibleRowCountMode="Auto" minAutoRowCount="10">
              <t:extension>
                <OverflowToolbar xmlns="sap.m" width="100%">
                  <ToolbarSpacer/>
                  <m:Button	icon="sap-icon://download" text="Exportar CSV" type="Transparent" press="onDataExport" />
                  <m:Button id="btnParcelContrato" text="{i18n>btnParcelItem}" type="Transparent" icon="sap-icon://split" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="onParcelItemContrato"/>
                  <m:Button id="btnDelContrato" text="Eliminar" type="Transparent" icon="sap-icon://delete" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="onDelContrato"/>
                </OverflowToolbar>
              </t:extension>
              <t:columns>
                <t:Column width="150px">
                  <Label xmlns="sap.m" text="{i18n>lblContrato}"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioContratoModel>Contrato}"/>
                  </t:template>
                </t:Column>
                <t:Column width="100px">
                  <Label xmlns="sap.m" text="{i18n>lblItemContrato}"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioContratoModel>Item}"/>
                  </t:template>
                </t:Column>
                <t:Column width="200px">
                  <Label xmlns="sap.m" text="Material acabado"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioContratoModel>MaterialAcab}" width="100px" />
                  </t:template>
                </t:Column>
                <t:Column width="310px">
                  <Label xmlns="sap.m" text="Descrição Material acabado" />
                  <t:template>
                    <Text xmlns="sap.m" text="{envioContratoModel>MaterialAcabDesc}" width="300px" />
                  </t:template>
                </t:Column>
                <t:Column width="200px">
                  <Label xmlns="sap.m" text="Componente"/>
                  <t:template>
                    <!-- <Text xmlns="sap.m" text="{envioContratoModel>MaterialComp}"/> -->
                    <m:Select showSecondaryValues= "true" wrapItemsText="true" width="180px" forceSelection="false"
                        selectedKey="{envioContratoModel>MaterialComp}" items="{envioContratoModel>Components}">
                      <core:ListItem key="{envioContratoModel>Idnrk}"
                          text="{envioContratoModel>Idnrk} - {envioContratoModel>Maktx}"
                          additionalText= "{envioContratoModel>Kmpmg} {envioContratoModel>Kmpme}" />
                    </m:Select>
                  </t:template>
                </t:Column>
                <!-- 
                <t:Column width="310px">
                  <Label xmlns="sap.m" text="Descrição componente"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioContratoModel>MaterialCompDesc}" width="300px" />
                  </t:template>
                </t:Column>
                -->
                <t:Column width="120px">
                  <Label xmlns="sap.m" text="Depósito"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioContratoModel>Deposito}"/>
                  </t:template>
                </t:Column>
                <!--
                <t:Column width="100px">
                  <Label xmlns="sap.m" text="{i18n>lblQtdContrato}"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{ path: 'envioContratoModel>QtdContrato', formatter: '.formatter.qtyValue' }"/>
                  </t:template>
                </t:Column>
                <t:Column width="5em">
                  <Label xmlns="sap.m" text="UM básica"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioContratoModel>UmBasica}"/>
                  </t:template>
                </t:Column>
                -->
                <t:Column width="100px">
                  <Label xmlns="sap.m" text="Qtde disponível"/>
                  <t:template>
                    <!--<Input xmlns="sap.m" id="fldQtdMatAcabadoManual" value="{ path: 'envioPedidoModel>QtdLote' }" type="Number"/>-->
                    <Input xmlns="sap.m" id="fldQtdMatAcabadoManualContrato" value="{path: 'envioContratoModel>QtdLote', type:'sap.ui.model.type.Float', formatOptions:{minFractionDigits:3,maxFractionDigits:3}, constraints:{precision:13,scale:3}}"/>
                  </t:template>
                </t:Column>
                <t:Column width="150px">
                  <Label xmlns="sap.m" text="Lote"/>
                  <t:template>
                    <Input id="inChargContrato"
                      xmlns="sap.m" value="{envioContratoModel>Lote}" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequestCharg">
                    </Input>
                  </t:template>
                </t:Column>
              </t:columns>
            </t:Table>
          </blocks>
        </ObjectPageSubSection>
      </ObjectPageSection>

    </sections>

    <footer>
      <m:OverflowToolbar>
        <m:Button id="messagePopoverBtn" visible="{=!!${message>/}.length}" icon="{ path: '/', formatter: '.buttonIconFormatter' }" type="{ path: '/', formatter: '.buttonTypeFormatter' }" text= "{path: '/', formatter: '.highestSeverityMessages' }" press=".handleMessagePopoverPress" />
        <m:ToolbarSpacer id="spacer1"/>
        <m:Button text="Enviar" id="btnEnviar" icon="sap-icon://create" press="onSave" />
        <m:Button text="Cancelar" id="btnCancelar" icon="sap-icon://sys-cancel" press="onCancelar" />
      </m:OverflowToolbar>
    </footer>

  </ObjectPageLayout>

