                  <Label xmlns="sap.m" text="Qtde envio UM"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioMaterialModel>QtdEnvioUn}"/>
                  </t:template>
                </t:Column>
                <t:Column width="150px">
                  <Label xmlns="sap.m" text="Pedido"/>
                  <t:template>
                    <Input xmlns="sap.m" maxLength="10" value="{envioMaterialModel>Pedido}" change=".onChangePedidoItem"/>
                  </t:template>
                </t:Column>
                <t:Column width="100px">
                  <Label xmlns="sap.m" text="Item Pedido"/>
                  <t:template>
                    <Input xmlns="sap.m" maxLength="5" value="{envioMaterialModel>Item}" change=".onChangePedidoItem" />
                  </t:template>
                </t:Column>
                <t:Column width="200px">
                  <Label xmlns="sap.m" text="Produto acabado"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioMaterialModel>Componente}"/>
                  </t:template>
                </t:Column>
              </t:columns>
            </t:Table>
          </blocks>
        </ObjectPageSubSection>
      </ObjectPageSection>

      
