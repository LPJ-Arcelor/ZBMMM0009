                <t:Column width="100px">
                  <Label xmlns="sap.m" text="Qtde disp. UM"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioMaterialModel>QtdDispUn}"/>
                  </t:template>
                </t:Column>
                <t:Column width="100px">
                  <Label xmlns="sap.m" text="Lote"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioMaterialModel>Lote}"/>
                  </t:template>
                </t:Column>
                <t:Column width="100px">
                  <Label xmlns="sap.m" text="Qtde envio"/>
                  <t:template>
                    <!--<Input xmlns="sap.m" value="{path:'envioMaterialModel>QtdEnvio' }" type="Number" required="true" />-->
                    <Input xmlns="sap.m" value="{path: 'envioMaterialModel>QtdEnvio', type:'sap.ui.model.type.Float', formatOptions:{minFractionDigits:3,maxFractionDigits:3}, constraints:{precision:13,scale:3}}"/>
