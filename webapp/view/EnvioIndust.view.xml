              </layout:HorizontalLayout>
            </m:VBox>
          </blocks>
        </ObjectPageSubSection>
        <ObjectPageSubSection id="sectionPedidoItems" title="{i18n>lblItems}" titleUppercase="false">
          <blocks>
            <t:Table id="ItensPedidoTable" rows="{ path: 'envioPedidoModel>/Itens' }" selectionMode="MultiToggle" fixedColumnCount="2" visibleRowCountMode="Auto" minAutoRowCount="10">
              <t:extension>
                <OverflowToolbar xmlns="sap.m" width="100%">
                  <ToolbarSpacer/>
                  <m:Button id="btnParcel" text="{i18n>btnParcelItem}" type="Transparent" icon="sap-icon://split" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="onParcelItem"/>
                  <m:Button id="btnDelPedido" text="Eliminar" type="Transparent" icon="sap-icon://delete" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="onDelPedido"/>
                </OverflowToolbar>
              </t:extension>
              <t:columns>
                <t:Column width="150px">
                  <Label xmlns="sap.m" text="Pedido"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioPedidoModel>Pedido}"/>
                  </t:template>
                </t:Column>
                <t:Column width="100px">
                  <Label xmlns="sap.m" text="Item Pedido"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioPedidoModel>Item}"/>
                  </t:template>
                </t:Column>
                <t:Column width="200px">
                  <Label xmlns="sap.m" text="Material acabado"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioPedidoModel>MaterialAcab}" width="100px" />
                  </t:template>
                </t:Column>
                <t:Column width="310px">
                  <Label xmlns="sap.m" text="Descrição Material acabado" />
                  <t:template>
                    <Text xmlns="sap.m" text="{envioPedidoModel>MaterialAcabDesc}" width="300px" />
                  </t:template>
                </t:Column>
                <t:Column width="200px">
                  <Label xmlns="sap.m" text="Componente"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioPedidoModel>MaterialComp}"/>
                  </t:template>
                </t:Column>
                <t:Column width="310px">
                  <Label xmlns="sap.m" text="Descrição componente"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioPedidoModel>MaterialCompDesc}" width="300px" />
                  </t:template>
                </t:Column>
                <t:Column width="120px">
                  <Label xmlns="sap.m" text="Depósito"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioPedidoModel>Deposito}"/>
                  </t:template>
                </t:Column>
                <t:Column width="100px">
                  <Label xmlns="sap.m" text="Qtde pedido"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{ path: 'envioPedidoModel>QtdPedido', formatter: '.formatter.qtyValue' }"/>
                  </t:template>
                </t:Column>
                <t:Column width="5em">
                  <Label xmlns="sap.m" text="UM básica"/>
                  <t:template>
                    <Text xmlns="sap.m" text="{envioPedidoModel>UmBasica}"/>
                  </t:template>
                </t:Column>
                <t:Column width="100px">
                  <Label xmlns="sap.m" text="Qtde disponível"/>
                  <t:template>
                    <!--<Input xmlns="sap.m" id="fldQtdMatAcabadoManual" value="{ path: 'envioPedidoModel>QtdLote' }" type="Number"/>-->
                    <Input xmlns="sap.m" id="fldQtdMatAcabadoManual" value="{path: 'envioPedidoModel>QtdLote', type:'sap.ui.model.type.Float', formatOptions:{minFractionDigits:3,maxFractionDigits:3}, constraints:{precision:13,scale:3}}"/>
                  </t:template>
                </t:Column>
                <t:Column width="150px">
                  <Label xmlns="sap.m" text="Lote"/>
                  <t:template>
                    <Input id="inChargPedido"
                      xmlns="sap.m" value="{envioPedidoModel>Lote}" showSuggestion="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequestCharg">
                    </Input>
                  </t:template>
                </t:Column>
              </t:columns>
            </t:Table>
          </blocks>
        </ObjectPageSubSection>
      </ObjectPageSection>

